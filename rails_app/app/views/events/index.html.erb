<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Channel</th>
      <th>Name</th>
      <th>Abstract</th>
      <th>Zoom url</th>
      <th>Host date</th>
      <th>From date</th>
      <th>To date</th>
      <th>Is delete</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.channel %></td>
        <td><%= event.name %></td>
        <td><%= event.abstract %></td>
        <td><%= event.zoom_url %></td>
        <td><%= event.host_date %></td>
        <td><%= event.from_date %></td>
        <td><%= event.to_date %></td>
        <td><%= event.is_delete %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
